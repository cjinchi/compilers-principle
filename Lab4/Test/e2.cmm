struct abc{
    int aa;
    int bb;
};

int show(struct abc myabc){
    write(myabc.aa);
    write(myabc.bb);
    write(myabc.aa+myabc.bb);
    return 0;
}

int theshow(int aaaa,int bbbb){
    write(aaaa);
    write(bbbb);
    return 0;
}

int main()
{
    struct abc theabc;
    theabc.aa = 534;
    theabc.bb = 619;
    theshow(123,345);
    return 0;
}